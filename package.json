{
  "name": "@nyren/codebase-setup",
  "version": "0.3.0",
  "description": "A CLI tool designed to streamline project setup for JavaScript and TypeScript, providing essential libraries and configurations for quick initialization and efficient workflows.",
  "bin": {
    "nyrenx-codeup": "dist/cli/nyrenx.js",
    "nyrenx": "dist/cli/nyrenx.js"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Saksorn21/nyren-ts-setup.git"
  },
  "author": "Boat Saksorn (Thailand)",
  "license": "MIT",
  "files": [
    "dist/*.js",
    "dist/cli/*.js",
    "dist/lib/*.js",
    "tsconfigs/*.json"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "cli": "bun src/cli/nyrenx.ts",
    "build": "tsc",
    "lint": "prettier --check 'src/**/*.{js,ts}' 'dist/**/*.{js,ts}'",
    "format": "prettier --write 'src/**/*.{js,ts}' 'dist/**/*.{js,ts}'",
    "test": "jest --coverage"
  },
  "keywords": [
    "cli",
    "command line",
    "setup",
    "project management",
    "tools",
    "nyren",
    "typescript",
    "javascript",
    "typescript setup",
    "javascript setup",
    "workflow automation",
    "repository",
    "configuration",
    "template"
  ],
  "bugs": {
    "url": "https://github.com/Saksorn21/nyren-ts-setup/issues"
  },
  "homepage": "https://github.com/Saksorn21/nyren-ts-setup#readme",
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.7.6",
    "@types/semver": "^7.5.8",
    "@types/which": "^3.0.4",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "5.6.3"
  },
  "dependencies": {
    "@inquirer/prompts": "^6.0.1",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "execa": "^9.4.1",
    "is-unicode-supported": "^2.1.0",
    "onetime": "^7.0.0",
    "ora": "^8.1.0",
    "semver": "^7.6.3",
    "which": "^5.0.0"
  }
}
